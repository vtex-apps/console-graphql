{
  "name": "console-graphql",
  "vendor": "vtex",
  "version": "0.1.0",
  "title": "IO Console API",
  "description": "VTEX developer console API",
  "mustUpdateAt": "2019-10-16",
  "scripts": {
    "postreleasy": "vtex publish --public"
  },
  "builders": {
    "node": "3.x",
    "graphql": "1.x"
  },
  "credential": "relative",
  "policies": [
    {
      "name": "read-workspace-apps"
    },
    {
      "name": "read-write-workspace"
    },
    {
      "name": "vbase-read-write"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.vtex.com",
        "path": "/api/storedash/{{account}}/metrics/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.myvtex.com",
        "path": "/*"
      }
    }
  ]
}
